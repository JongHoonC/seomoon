<form action="/night-sign-up" method="post" onsubmit="return Validation();">
  <div class="signUp">
    <div class="top_signUp">
      <h2>회원가입</h2>
      <ul>
        <div class="name">
          <li><label for="userName">이름 : </label></li>
          <li><input type="text" name="userName" id="userName" /></li>
        </div>
        <div class="id">
          <li><label for="userID">아이디 : </label></li>
          <li><input type="text" name="userID" id="userID" /></li>
        </div>
        <div class="pw">
          <li><label for="userPW">비밀번호 : </label></li>
          <li><input type="password" name="userPW" id="userPW" /></li>
        </div>
        <div class="conpw">
          <li><label for="conpw">비밀번호 확인 : </label></li>
          <li><input type="password" name="conpw" id="conpw" /></li>
        </div>
      </ul>
      <input type="submit" id="GOLOGIN" value="회원가입" class="ko" />
    </div>
  </div>
</form>
